  object IBQcolocacion: TIBQuery
    Database = dmGeneral.IBDatabase1
    Transaction = IBTproceso
    SQL.Strings = (
      
        'SELECT c.*, (p.NOMBRE || '#39' '#39' || p.PRIMER_APELLIDO || '#39' '#39' || p.SE' +
        'GUNDO_APELLIDO) AS NOMBRE FROM "col$colocacion" c'
      
        'INNER JOIN "gen$persona" p ON p.ID_IDENTIFICACION = c.ID_IDENTIF' +
        'ICACION and p.ID_PERSONA = c.ID_PERSONA'
      
        'WHERE c.ID_LINEA NOT IN (8,13,14) AND c.ID_ESTADO_COLOCACION IN ' +
        '(0,1,2)'
      'ORDER BY c.ID_COLOCACION ASC')
    Left = 96
    Top = 56
  end
  object IBQajustar: TIBQuery
    Database = dmGeneral.IBDatabase1
    Transaction = IBTproceso
    SQL.Strings = (
      
        'UPDATE "col$colocacion" SET TASA_INTERES_CORRIENTE = :TASA_INTER' +
        'ES_CORRIENTE, VALOR_CUOTA = :VALOR_CUOTA WHERE ID_COLOCACION = :' +
        'ID_COLOCACION')
    Left = 152
    Top = 56
    ParamData = <
